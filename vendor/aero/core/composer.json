{
    "name": "aero/core",
    "description": "Aero Core Package - Foundation for all Aero modules including User, Auth, Roles, Permissions, Modules, and Multi-tenancy",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "aero",
        "core",
        "laravel",
        "multi-tenancy",
        "authentication",
        "authorization",
        "rbac",
        "module-system"
    ],
    "authors": [
        {
            "name": "Aero Team",
            "email": "support@aerosuite.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "spatie/laravel-permission": "^6.20",
        "spatie/laravel-medialibrary": "^11.0",
        "inertiajs/inertia-laravel": "^2.0",
        "tightenco/ziggy": "^2.0",
        "jenssegers/agent": "^2.6|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "laravel/boost": "*",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "suggest": {
        "stancl/tenancy": "Required for multi-tenancy support (^3.9)",
        "laravel/fortify": "Required for authentication features (^1.24)",
        "laravel/sanctum": "Required for API authentication (^4.0)",
        "spatie/laravel-medialibrary": "Required for media uploads (^11.0)"
    },
    "autoload": {
        "psr-4": {
            "Aero\\Core\\": "src/",
            "Aero\\Core\\Database\\Factories\\": "database/factories/",
            "Aero\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aero\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aero\\Core\\AeroCoreServiceProvider"
            ],
            "aliases": {
                "AeroCore": "Aero\\Core\\Facades\\AeroCore"
            }
        },
        "aero": {
            "package": "core",
            "version": "1.0.0",
            "category": "foundation",
            "description": "Foundation package providing User, Auth, Roles, Permissions, Modules"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}


