{
    "name": "aero/ui",
    "description": "Aero UI Package - Shared React components, hooks, and utilities for all Aero modules",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "aero",
        "ui",
        "react",
        "components",
        "heroui",
        "tailwind"
    ],
    "authors": [
        {
            "name": "Aero Team",
            "email": "support@aerosuite.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Aero\\UI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aero\\UI\\AeroUIServiceProvider"
            ]
        },
        "aero": {
            "package": "ui",
            "version": "1.0.0",
            "category": "foundation",
            "description": "Shared React component library for all Aero modules"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
